<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rajendra.onlinedailygroceries">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CustomerView"></activity>
        <activity android:name=".AddSellerProd" />
        <activity android:name=".crudseller" />
        <activity android:name=".sellercreateaccount" />
        <activity android:name=".sellerslogin" />
        <activity android:name=".CreateAcc" />
        <activity android:name=".login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Checkout" />
        <activity android:name=".Cart" />
        <activity android:name=".AllProduct" />
        <activity android:name=".ProductDetails" />
        <activity android:name=".AllCategory" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".ProductList"
            android:label="Food List"
            android:parentActivityName=".AddSellerProd" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>